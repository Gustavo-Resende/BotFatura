# Connection Strings
# Formato: Host=HOST;Port=PORT;Database=DATABASE;Username=USER;Password=PASSWORD
ConnectionStrings__DefaultConnection=Host=127.0.0.1;Port=5433;Database=botfatura_db;Username=admin;Password=SUA_SENHA_SEGURA

# Evolution API
EvolutionApi__BaseUrl=http://localhost:8080/
EvolutionApi__InstanceName=BotFatura
EvolutionApi__ApiKey=SUA_API_KEY_EVOLUTION
EvolutionApi__WebhookSecret=SEU_WEBHOOK_SECRET

# Default Admin
DefaultAdmin__Email=admin@botfatura.com.br
DefaultAdmin__Password=SUA_SENHA_ADMIN_SEGURA

# JWT Settings
# IMPORTANTE: A chave secreta deve ter no mÃ­nimo 32 caracteres
JwtSettings__Secret=SUA_CHAVE_SECRETA_JWT_MINIMO_32_CARACTERES
JwtSettings__Issuer=BotFaturaApi
JwtSettings__Audience=BotFaturaFrontend
JwtSettings__ExpiryInMinutes=1440

# Gemini API
GeminiApi__ApiKey=SUA_CHAVE_GEMINI_API
GeminiApi__Model=gemini-2.5-flash
GeminiApi__MaxFileSizeMB=6

# API Base URL
API_BASE_URL=http://localhost:5000
